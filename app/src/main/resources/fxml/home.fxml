<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<StackPane fx:id="homePage" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.easya.assetmanager.ui.controller.HomeController">
    <HBox fx:id="homeContent" alignment="CENTER_LEFT" spacing="10" maxWidth="Infinity" maxHeight="Infinity">
        <padding>
            <Insets top="0" right="10" bottom="10" left="10"/>
        </padding>
        <ImageView fx:id="homeImage" fitHeight="300" preserveRatio="true">
            <image>
                <Image url="/images/home.png" />
            </image>
        </ImageView>
    </HBox>
</StackPane> 